{
  "cli": {
    "version": ">= 3.4.1"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m1-medium"
      },
      "env": {
        "CG_URL": "https://dev.caregiver.pinwheel.com/#/"
      },
      "channel": "development"
    },
    "development-preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m1-medium"
      },
      "env": {
        "CG_URL": "https://dev.caregiver.pinwheel.com/#/"
      },
      "channel": "development-preview"
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m1-medium"
      },
      "env": {
        "CG_URL": "https://caregiver.pinwheel.com/#/"
      },
      "channel": "preview"
    },
    "production": {
      "ios": {
        "resourceClass": "m1-medium"
      },
      "env": {
        "CG_URL": "https://caregiver.pinwheel.com/#/"
      },
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
